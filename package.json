{
  "name": "powefit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "bash ./scripts/app/run-development.sh",
    "build": "bash ./scripts/app/build-production.sh",
    "postinstall": "bash ./scripts/hooks/package/post-install.sh",
    "prepare": "npm run tool:husky:init",
    "lint": "npm run tool:lint:uncache && npm run tool:lint:execute:dev",
    "format": "npm run tool:format:code && npm run tool:format:scripts",
    "tool:app:env-check": "bash ./scripts/app/check-env.sh",
    "tool:app:purge-cache": "bash ./scripts/app/cloudflare-uncache.sh",
    "tool:husky:init": "bash ./scripts/tools/init-git-hooks.sh",
    "tool:sum-hash:build": "bash ./scripts/tools/sumhash-build.sh",
    "tool:sum-hash:check": "bash ./scripts/tools/sumhash-check.sh",
    "tool:lint:uncache": "bash ./scripts/tools/eslint-uncache.sh",
    "tool:lint:execute:dev": "bash ./scripts/tools/eslint-execute.sh dev",
    "tool:lint:execute:prod": "bash ./scripts/tools/eslint-execute.sh prod",
    "tool:format:code": "bash ./scripts/tools/format-code.sh",
    "tool:format:scripts": "bash ./scripts/tools/format-scripts.sh"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "next": "^13.0.2",
    "next-i18next": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.1",
    "@svgr/webpack": "^6.5.1",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-sh": "^0.12.8"
  }
}
